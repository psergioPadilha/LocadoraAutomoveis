@model DetalhesClienteViewModel

<div class="row gap-3 my-3 my-xl-5">
	<label class="fs-4"><strong>Nome:</strong> @Model.Nome</label>

	<label class="fs-4"><strong>Tipo de Cadastro:</strong> @Model.TipoCliente</label>

	<label class="fs-4"><strong>Número do Documento:</strong> @Model.NumeroDocumento</label>

	<label class="fs-4"><strong>Telefone:</strong> @Model.Telefone</label>

	<label class="fs-4"><strong>Email:</strong> @Model.Email</label>

	<label class="fs-5"><strong>Id:</strong> [@Model.Id]</label>
</div>